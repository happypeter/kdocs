#include <stdio.h>
#include <fcntl.h>
int main()
{
int fd;
char buf[256];
unsigned long tmp;
fd=open("/dev/leds",O_RDWR);
if(fd<0)
{
perror("open /dev/leds\n");
exit(1);
}
read(fd,buf,4);
tmp=buf[0]+(buf[1]<<8)+(buf[2]<<16)+(buf[3]<<24);
printf("read-------GPFDAT=0x%lx\n",tmp);
close(fd);
return 0;
} 
